<Window x:Class="DemoProblem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoProblem"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Width="430" Content="Browse" Command="{Binding CommandInViewModel}" Click="ButtonBase_OnClick" Margin="180,0,0,352" Height="21" VerticalAlignment="Bottom" />
        </StackPanel>
        <DataGrid Name="DataGrid1" ItemsSource="{Binding Data}" Margin="0,75,0,0"/>
    </Grid>
</Window>
